	"source": "lib/binarypack.ts",
	"types": "dist/binarypack.d.ts",
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/binarypack.d.ts",
				"default": "./dist/binarypack.mjs"
